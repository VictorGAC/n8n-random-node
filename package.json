{
  "name": "n8n-nodes-random",
  "version": "0.1.0",
  "description": "True random number generator node for n8n using Random.org",
  "keywords": [
    "n8n",
    "n8n-node",
    "random"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "icons"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "dev": "tsc -w -p tsconfig.json",
    "lint": "eslint \"nodes/**/*.ts\"",
    "prepare": "npm run build"
  },
  "dependencies": {
    "n8n-workflow": "^1.110.0",
    "tslib": "^2.7.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.5.4"
  },
  "n8n": {
    "n8nNodes": [
      {
        "name": "Random.node.js",
        "className": "Random",
        "sourcePath": "dist/nodes/Random/Random.node.js"
      }
    ],
    "n8nCredentials": []
  }
}
